{
  "name": "bandronic/oauth2server",
  "description": "OAuth2 server based on BShaffer OAuth2, utilizing Doctrine",
  "minimum-stability": "stable",
  "keywords":["oauth","oauth2","auth","doctrine"],
  "type":"library",
  "license": "MIT",
  "authors": [
    {
      "name": "Bogdan A.",
      "email": "bandronic@gmail.com"
    }
  ],
  "require":{
    "php": "^5.6 || ^7.0",
    "bshaffer/oauth2-server-php": "~1.8",
    "dasprid/container-interop-doctrine": "1.0.0",
    "robmorgan/phinx": "^0.9.1",
    "symfony/http-foundation": "^3.3",
    "zendframework/zend-config-aggregator": "^1.0",
    "zendframework/zend-expressive": "^2.0.2",
    "zendframework/zend-servicemanager": "^3.3",
    "http-interop/http-server-handler": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "OAuth2Server\\": "src/"
    }
  }
}
